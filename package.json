{
  "name": "cnn-hapi",
  "version": "0.2.0",
  "description": "This is a generic Hapi server for starting projects.",
  "main": "main.js",
  "scripts": {
    "generate-changelog": "changelog-maker --group",
    "generate-docs": "rm -rf docs && esdoc -c .esdoc.json",
    "test": "eslint *.js **/*.js && test/validate-json-files.sh",
    "update-apply": "ncu -u",
    "update-check": "ncu"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cnnlabs/cnn-hapi"
  },
  "bugs": {
    "url": "https://github.com/cnnlabs/cnn-hapi/issues"
  },
  "homepage": "https://github.com/cnnlabs/cnn-hapi#readme",
  "engines": {
    "node": ">=4.2.6"
  },
  "dependencies": {
    "boom": "3.1.2",
    "cnn-health": "0.0.2",
    "cnn-metrics": "0.2.0",
    "debounce": "1.0.0",
    "debug": "2.2.0",
    "engine.io": "1.6.8",
    "fetchres": "1.5.1",
    "good": "6.4.0",
    "handlebars": "4.0.5",
    "hapi": "12.1.0",
    "hapi-swagger": "4.0.0",
    "hoek": "3.0.4",
    "inert": "3.2.0",
    "isomorphic-fetch": "2.2.1",
    "joi": "7.2.3",
    "socket.io": "1.4.5",
    "vision": "4.0.1"
  },
  "devDependencies": {
    "babel-cli": "6.4.5",
    "babel-preset-es2015": "6.3.13",
    "changelog-maker": "2.2.0",
    "esdoc": "0.4.3",
    "eslint": "1.10.3",
    "jq-cli-wrapper": "0.3.0",
    "jsonlint": "1.6.2",
    "npm-check-updates": "2.5.8"
  },
